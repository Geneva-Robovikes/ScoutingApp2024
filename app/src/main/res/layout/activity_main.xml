<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dataLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#23132e"
    android:screenOrientation="landscape"
    tools:context=".MainActivity">
    <!--Header-->
    <pl.droidsonroids.gif.GifImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleX="1.1"
        android:scaleY="1.1"
        android:src="@drawable/gif_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5">

    </pl.droidsonroids.gif.GifImageView>

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_35sdp"
            android:background="#431e5c"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:padding="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/TESTINGTITLE"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/robovikes_3067_scouting_app"
                android:textColor="@color/white"
                android:textSize="36sp" />
            <Space
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1">
            </Space>
            <Button
                android:id="@+id/resetButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/reset"
                android:layout_margin="2dp"
                android:textColor="@color/white">
            </Button>
            <ImageButton
                android:id="@+id/buttonToTeams"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:layout_margin="2dp"
                android:src="@drawable/chooseteamicon">
            </ImageButton>
            <ImageButton
                android:id="@+id/mapButtonToMap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:src="@drawable/map_foreground"
                android:contentDescription="@string/tomap">
            </ImageButton>
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="32dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:padding="24dp">

            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:background="#431e5c"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="4dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/teamnumber"
                        android:textColor="@color/white"
                        android:textSize="24sp">
                    </TextView>
                    <EditText
                        android:id="@+id/editTeamName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/enter_teamname"
                        android:inputType="number"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white">
                    </EditText>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/matchnumber"
                        android:textColor="@color/white"
                        android:textSize="24sp">
                    </TextView>
                    <EditText
                        android:id="@+id/matchnumber"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/matchnumber"
                        android:inputType="number"
                        android:textColor="@color/white"
                        android:textColorHint="@color/white">
                    </EditText>
                </LinearLayout>

                <RadioGroup
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <RadioButton
                        android:id="@+id/blueteam"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/blue_team"
                        android:textColor="#0000FF"
                        android:textSize="24sp">
                    </RadioButton>
                    <RadioButton
                        android:id="@+id/redteam"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/red_team"
                        android:textColor="#FF0000"
                        android:textSize="24sp">
                    </RadioButton>
                </RadioGroup>
            </LinearLayout>
            <Button
                android:id="@+id/generateQRCODE_match"
                android:layout_width="200dp"
                android:layout_height="100dp"
                android:text="@string/generate_match_data"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">
            </Button>
            <Button
                android:id="@+id/generateQRCODE_pit"
                android:layout_width="200dp"
                android:layout_height="100dp"
                android:text="@string/generate_pit_data"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5">
            </Button>

        </LinearLayout>
        <LinearLayout
            android:layout_width="@dimen/_300sdp"
            android:layout_height="@dimen/_200sdp"
            android:layout_margin="32dp"
            android:background="#CC431e5c"
            android:orientation="vertical"
            android:padding="24dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="#431e5c"
                android:orientation="horizontal"
                android:paddingHorizontal="64dp">

                <Button
                    android:id="@+id/toPitButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#F5F0F6"
                    android:text="@string/pit"
                    android:textColor="@color/white"
                    android:textSize="24sp" />

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/toAutoButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#99810E44"
                    android:text="@string/auto"
                    android:textColor="@color/white"
                    android:textSize="24sp" />

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/toTeleopButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#2B2D42"
                    android:text="@string/teleop"
                    android:textColor="@color/white"
                    android:textSize="24sp" />
            </LinearLayout>

            <ScrollView
                android:id="@+id/pitData"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#33F5F0F6"
                android:padding="32dp">

                <TableLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#33F5F0F6">
                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/robot_width_w_o_bumper_in"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <EditText
                            android:id="@+id/robotWidth"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="32dp"
                            android:hint="@string/enter"
                            android:inputType="numberDecimal"
                            android:textAlignment="center"
                            android:textColorHint="@color/white"
                            android:textSize="20sp" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/robot_width_w_bumper_in"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                        </Space>

                        <EditText
                            android:id="@+id/robotWidthBumper"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="32dp"
                            android:hint="@string/enter"
                            android:inputType="numberDecimal"
                            android:textAlignment="center"
                            android:textColorHint="@color/white"
                            android:textSize="20sp" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/robot_length_w_o_bumper_in"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <EditText
                            android:id="@+id/robotLength"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="32dp"
                            android:hint="@string/enter"
                            android:inputType="numberDecimal"
                            android:textAlignment="center"
                            android:textColorHint="@color/white"
                            android:textSize="20sp" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/robot_length_w_bumper_in"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <EditText
                            android:id="@+id/robotLengthBumper"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="32dp"
                            android:hint="@string/enter"
                            android:inputType="numberDecimal"
                            android:textAlignment="center"
                            android:textColorHint="@color/white"
                            android:textSize="20sp" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/cycle_time_s"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <EditText
                            android:id="@+id/cycleTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="32dp"
                            android:hint="@string/enter"
                            android:inputType="numberDecimal"
                            android:textAlignment="center"
                            android:textColorHint="@color/white"
                            android:textSize="20sp" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/drive_type"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <EditText
                            android:id="@+id/driveType"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="32dp"
                            android:hint="@string/enter"
                            android:inputType="text"
                            android:textAlignment="center"
                            android:textColorHint="@color/white"
                            android:textSize="20sp" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/can_hang_on_chain"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <CheckBox
                            android:id="@+id/pitChain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/can_score_in_speaker"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <CheckBox
                            android:id="@+id/pitSpeaker"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/can_score_in_amp"
                            android:textColor="@color/white"
                            android:textSize="24sp" />
                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />
                        <CheckBox
                            android:id="@+id/pitAmp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/can_pick_up_notes_from_ground"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <CheckBox
                            android:id="@+id/pitGroundNotes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/can_pick_up_notes_from_loading"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1" />

                        <CheckBox
                            android:id="@+id/pitLoadingNotes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </TableRow>
                </TableLayout>
            </ScrollView>

            <ScrollView
                android:id="@+id/autoData"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#99810E44"
                android:padding="32dp"
                android:visibility="gone">

                <TableLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/notes_in_amp_scored"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="500dp"
                            android:layout_height="0dp"
                            android:layout_weight="1" />

                        <Button
                            android:id="@+id/autoAmpsScoredMinus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/minus"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <TextView
                            android:id="@+id/autoAmpsScored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="16dp"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Button
                            android:id="@+id/autoAmpsScoredPlus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/plus"
                            android:textColor="@color/white"
                            android:textSize="24sp"/>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Notes in Speaker Scored"
                            android:textColor="@color/white"
                            android:textSize="24sp" />

                        <Space
                            android:layout_width="500dp"
                            android:layout_height="0dp"
                            android:layout_weight="1" />

                        <Button
                            android:id="@+id/autoSpeakerScoredMinus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/minus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>

                        <TextView
                            android:id="@+id/autoSpeakerScored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="16dp"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Button
                            android:id="@+id/autoSpeakerScoredPlus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/plus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Left Starting Area"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"></Space>

                        <CheckBox
                            android:id="@+id/autoLeftArea"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"></CheckBox>
                    </TableRow>
                </TableLayout>
            </ScrollView>

            <ScrollView
                android:id="@+id/teleopData"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#992B2D42"
                android:padding="32dp"
                android:visibility="gone">

                <TableLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Notes in Amp Scored"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="500dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"></Space>

                        <Button
                            android:id="@+id/AmpsScoredMinus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/minus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>

                        <TextView
                            android:id="@+id/AmpsScored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="16dp"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Button
                            android:id="@+id/AmpsScoredPlus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/plus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Notes in Speaker Scored"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="500dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"></Space>

                        <Button
                            android:id="@+id/SpeakerScoredMinus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/minus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>

                        <TextView
                            android:id="@+id/SpeakerScored"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="16dp"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Button
                            android:id="@+id/SpeakerScoredPlus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/plus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Shots Missed"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="500dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"></Space>

                        <Button
                            android:id="@+id/ShotsMissedMinus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/minus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>

                        <TextView
                            android:id="@+id/ShotsMissed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="16dp"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Button
                            android:id="@+id/ShotsMissedPlus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/plus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>
                    </TableRow>
                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Feeder Shots"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="500dp"
                            android:layout_height="0dp"
                            android:layout_weight="1"></Space>

                        <Button
                            android:id="@+id/FeederShotsMinus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/minus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>

                        <TextView
                            android:id="@+id/FeederShotsView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="16dp"
                            android:text="0"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Button
                            android:id="@+id/FeederShotsPlus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleX="0.8"
                            android:scaleY="0.8"
                            android:text="@string/plus"
                            android:textColor="@color/white"
                            android:textSize="24sp"></Button>
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Completed Climb"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"></Space>

                        <CheckBox
                            android:id="@+id/Climb"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"></CheckBox>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Completed Trap"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"></Space>

                        <CheckBox
                            android:id="@+id/Trap"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"></CheckBox>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Broke down, can't be restarted"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"></Space>

                        <CheckBox
                            android:id="@+id/BrokeDownFully"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"></CheckBox>
                    </TableRow>

                    <TableRow>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Broke down, but restarted"
                            android:textColor="@color/white"
                            android:textSize="24sp"></TextView>

                        <Space
                            android:layout_width="300dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"></Space>

                        <CheckBox
                            android:id="@+id/BrokeDownRestarted"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"></CheckBox>
                    </TableRow>
                    <TableRow
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Defense Score (1-10)"
                            android:textColor="@color/white"
                            android:textSize="24sp">
                        </TextView>
                        <EditText
                            android:id="@+id/DefenseScore"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:hint="@string/enter"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:inputType="number">
                        </EditText>
                    </TableRow>
                </TableLayout>
            </ScrollView>
        </LinearLayout>
    </LinearLayout>


    <ImageView
        android:id="@+id/QRCODE"
        android:layout_width="@dimen/_200sdp"
        android:layout_height="@dimen/_200sdp"
        android:background="@color/black"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"></ImageView>

    <ImageView
        android:id="@+id/cat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@null"
        android:scaleX="3"
        android:scaleY="3"
        android:src="@drawable/cat"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"></ImageView>

    <ImageView
        android:id="@+id/fnaf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@null"
        android:scaleX="1.1"
        android:scaleY="1.1"
        android:src="@drawable/fnaf"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"></ImageView>

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/amongus"
        android:layout_width="573dp"
        android:layout_height="322dp"
        android:src="@drawable/amonggus"
        android:scaleY="2.3"
        android:scaleX="2.3"
        android:translationZ="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        android:visibility="gone">
    </pl.droidsonroids.gif.GifImageView>
</androidx.constraintlayout.widget.ConstraintLayout>